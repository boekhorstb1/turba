{
    "name": "horde/turba",
    "description": "Address book application",
    "type": "horde-application",
    "homepage": "https://www.horde.org/apps/turba",
    "license": "ASL",
    "authors": [
        {
            "name": "Michael J Rubinsky",
            "email": "mrubinsk@horde.org",
            "role": "lead"
        },
        {
            "name": "Jan Schneider",
            "email": "jan@horde.org",
            "role": "lead"
