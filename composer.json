            "email": "chuck@horde.org",
            "role": "lead"
        }
    ],
    "version": "5.0.0",
    "time": "2020-10-27",
    "repositories": [
        {
            "type": "composer",
            "url": "https://horde-satis.maintaina.com"
        }
    ],
    "require": {
        "horde/horde-installer-plugin": "dev-master",
        "php": "^5.3 || ^7",
        "horde/content": "dev-master",
        "horde/horde": "dev-master",
        "horde/auth": "dev-master",
        "horde/core": "dev-master",
        "horde/data": "dev-master",
        "horde/date": "dev-master",
        "horde/dav": "dev-master",
        "horde/exception": "dev-master",
        "horde/form": "dev-master",
        "horde/group": "dev-master",
        "horde/history": "dev-master",
        "horde/icalendar": "dev-master",
        "horde/mail": "dev-master",
        "horde/mime": "dev-master",
        "horde/nls": "dev-master",
        "horde/perms": "dev-master",
        "horde/prefs": "dev-master",
        "horde/serialize": "dev-master",
        "horde/share": "dev-master",
        "horde/support": "dev-master",
        "horde/url": "dev-master",
        "horde/util": "dev-master",
        "horde/vfs": "dev-master",
        "horde/view": "dev-master",
        "ext-gettext": "*",
        "ext-hash": "*",
        "ext-json": "*"
    },
    "suggest": {
        "horde/activesync": "^2.4",
        "horde/backup": "^1",
        "horde/db": "^2.0.3",
        "horde/imsp": "^2",
        "horde/kolab_format": "^2.0.5",
        "horde/kolab_storage": "^2.1",
        "horde/ldap": "^2.1",
        "horde/openxchange": "^1"
    },
    "autoload": {
        "psr-0": {
            "Turba": "lib/"
        }
    },
    "bin": [
        "bin/turba-convert-sql-shares-to-sqlng",
        "bin/turba-import-openxchange",
        "bin/turba-import-vcards",
        "bin/turba-import-squirrelmail-sql-abook",
        "bin/turba-import-squirrelmail-file-abook",
        "bin/turba-public-to-horde-share",
        "bin/turba-convert-datatree-shares-to-sql",
        "bin/turba-validate-email"
    ]
